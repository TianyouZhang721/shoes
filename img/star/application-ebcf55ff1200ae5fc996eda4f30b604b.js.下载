assword.init(),app.productOrderApply.init(),app.product.init(),app.productShow.init(),app.haiTaoProductShow.init(),app.myOrder.init(),app.productOrderNew.init(),app.productOrderCheckout.init(),app.myOrderShow.init(),app.lotteries.init(),app.lotteryShow.init(),app.comment.init(),app.evaluation.init(),app.evaluationCreate.init(),app.evaluationShow.init(),app.appraise.init(),app.appraiseShow.init(),app.appraiseSellerShow.init(),app.search.init(),app.feedAutoHeight.init(),app.feedShow.init(),app.feedTopic.init(),app.feedAddTopic.init(),app.userTab.init(),app.userPannel.init(),app.personalCenter.init(),app.communityNew.init(),app.videoShow.init(),app.newIdCard.init(),app.myCoupon.init(),app.orderAddress.init(),app.htProductOrderNew.init(),app.productSearch.init(),app.myPhone.init(),app.myAvator.init(),app.shoeOrder.init()});